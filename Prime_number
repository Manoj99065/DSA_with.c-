#include<iostream>
using namespace std;
int prime(int n){
int count=0;
   for(int i=1;i<=n;i++)
   {
      if(n%i==0)
      {
         count=count+1;
      }
   }
   if(count==2)
   return n;
   }

int main()
{
   
   int r;
   cout<<"Enter the value of the redius = ";
   cin>>r;
   cout<<"Yes , The given number is pirme "<<prime(r);
return 0;
}
