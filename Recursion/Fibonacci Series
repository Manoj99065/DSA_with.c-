#include<iostream>
using namespace std;
int fact(int a)
{
   if(a==0)
   {
      return  1;
   }
   static int j=1;
   static int i=0;
   int bigger=fact(a-1);
   int temp=i;
   cout<<i<<endl;
   i=i+j;
   j=temp;
}

int main()
{
   int a;
   cout<<"Enter the value for power = ";
   cin>>a;
   fact(a);
   return 0;
}
