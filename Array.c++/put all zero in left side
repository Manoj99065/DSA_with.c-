#include<iostream>
using namespace std;
int main()
{
   int arr[]={0,1,0,3,0,13,15};
   int j=0;
   int size=sizeof(arr)/sizeof(arr[0]);
   for(int i=0;i<size;i=i+1)
   {
      if(arr[i]!=0)
      {
         swap(arr[j],arr[i]);
         j=j+1;
      }
   }
   for(int i=0;i<size;i++)
   {
      cout<<arr[i];
      if(i!=size-1)
      {
         cout<<",";
      }
   }
}
