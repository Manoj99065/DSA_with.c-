#include<iostream>
#include<string>
using namespace std;
int main()
{
   string m="manoj rana";
   char arr[100];
   // int count=1;
   int size=m.size();
   int t=0;
   char arr1[100];
   for(int i=0;i<size;i=i+1)
   {
      arr[i]=m[i];
   }
   for(int i=0;i<size;i=i+1)
   {
      int count=1;
      // arr[i]=m[i];
      for(int j=i+1;j<size;j++)
      {
        if(arr[i]==arr[j])
        {
         count=count+1;
      for(int k=j;k<size-1;k=k+1)
      {
       arr[k]=arr[k+1];
      }
      size=size-1;
      j=j-1;
   }  
   }
   arr1[t++]=arr[i];
   arr1[t++]='0'+count;
   
}

arr1[t]='\0';

cout<<arr1<<endl;
return 0;
}
