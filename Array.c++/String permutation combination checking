#include<iostream>
#include<string>
#include<algorithm>
using namespace std;
int main()
{
   string a="ab";
   string A=a;
   reverse(A.begin(),A.end());
   string b="eibaooo";
   int size3=b.size();
   for(int i=0;i+1<size3;i++)
   {
   string block=b.substr(i,2);
   if(a==block || A==block)
   {
      cout<<block;
   }
   }
   return 0;
}
