#include<iostream>
using namespace std;
int main()
{
   int arr[]={1,2,3,4,5,6,7};
   int size=sizeof(arr)/sizeof(arr[0]);
   int k=3;
   int temp;
   for(int i=0;i<k;i=i+1)
   {
      temp=arr[size-1];
      // cout<<arr[size-1];
      for(int j=size-1;j>=0;j=j-1)
      {
        arr[j]=arr[j-1];
      }
      arr[0]=temp;
   }
   // in this section we are display arrays.
   for(int i=0;i<size;i++)
   {
      cout<<arr[i];
      if(i!=size-1)
      {
         cout<<",";
      }
   }
return 0;
}
