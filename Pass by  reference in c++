#include<iostream>
using namespace std;
class manoj
{
   public :
   void Arr(int arr1[],int size)
   {
      for(int i=0;i<size;i++)
      {
       cout<<arr1[i]*2;
       if(i!= size-1)
       {
         cout<<",";
       }
      }
      delete []arr1;
   }
};
int main()
{
   int arr[]={1,2,3};
   manoj a1;
   int size=3;
   a1.Arr(arr,size);
   return 0;
}
